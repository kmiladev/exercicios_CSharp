using System;

internal class Program
{
    private static void Main(string[] args)
    {
        {
            Console.WriteLine("Digite um n√∫mero para consultar sua tabuada");
            int numeroDigitado = int.Parse(Console.ReadLine());

            for (int i = 1; i <= 10; i++)
            {
                Console.WriteLine("{0} X {1} = {2}", numeroDigitado, i, (numeroDigitado * i));
            }

            Console.WriteLine("Fim do Programa!");

        }
    }
}
